{
  "name": "@synnaxlabs/media",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "description": "Media Assets for Synnax Labs",
  "repository": "https://github.com/synnaxlabs/synnax/tree/main/x/media",
  "keywords": [
    "synnax",
    "media",
    "data acquisition",
    "big data",
    "telemetry",
    "control syustems"
  ],
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "test": "vitest",
    "cov": "vitest --coverage",
    "lint": "eslint \"src/**/*.ts*\"",
    "fix": "eslint \"src/**/*.ts*\" --fix"
  },
  "dependencies": {
    "@synnaxlabs/x": "workspace:*",
    "@types/react": "^18.2.31",
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0"
  },
  "devDependencies": {
    "@synnaxlabs/tsconfig": "workspace:*",
    "@synnaxlabs/vite-plugin": "workspace:*",
    "@types/node": "^20.8.7",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint-config-synnaxlabs": "workspace:*",
    "vite": "^5.1.2",
    "vitest": "^1.2.2"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/media.cjs.js",
  "module": "./dist/media.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/media.es.js",
      "require": "./dist/media.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": "./dist/*"
  }
}
