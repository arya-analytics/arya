{
  "name": "@synnaxlabs/media",
  "version": "0.29.0",
  "type": "module",
  "description": "Media Assets for Synnax Labs",
  "repository": "https://github.com/synnaxlabs/synnax/tree/main/x/media",
  "keywords": [
    "synnax",
    "media",
    "data acquisition",
    "big data",
    "telemetry",
    "control syustems"
  ],
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "test": "vitest",
    "cov": "vitest --coverage",
    "lint": "eslint \"src/**/*.ts*\"",
    "fix": "eslint \"src/**/*.ts*\" --fix"
  },
  "dependencies": {
    "@synnaxlabs/x": "workspace:*",
    "@types/react": "^18.3.3",
    "clsx": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1"
  },
  "devDependencies": {
    "@synnaxlabs/tsconfig": "workspace:*",
    "@synnaxlabs/vite-plugin": "workspace:*",
    "@types/node": "^20.14.9",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint-config-synnaxlabs": "workspace:*",
    "vite": "5.4.4",
    "vitest": "^2.1.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/media.cjs",
  "module": "./dist/media.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/media.js",
      "require": "./dist/media.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": "./dist/*"
  }
}
