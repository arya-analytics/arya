    "electron": "^30.0.6",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-synnaxlabs": "workspace:*",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "3.2.5",
    "stylelint-config-synnaxlabs": "workspace:*",
    "typescript": "^5.4.5",
    "vite": "5.1.2",
    "vite-plugin-electron": "^0.28.7",
    "vite-tsconfig-paths": "4.3.1"
