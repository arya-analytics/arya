{
  "name": "@synnaxlabs/console",
  "private": true,
  "version": "0.21.7",
  "main": "./dist-electron/main.js",
  "author": "Synnax Labs",
  "description": "Synnax Console",
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
