        .synnax-logo {
            height: 42px;
            fill: white;
        }
        .title::after {
            content: "";
            display: block;
            width: 50%;
            margin-top: 1rem;
            height: 3px;
            background: var(--pluto-primary-z);
        }
        .hero-parent {
            height: 75vh;
            .hero {
                width: 1100px;
            }
        }
        .guides {
