---
import PageNav from "@/components/PageNav/PageNav.astro";
import Root from "@/layouts/Root.astro";
import OneBillionRowsTile from "@/pages/guides/comparison/one-billion-rows/OneBillionRowsTile.astro";
import ReleaseTile from "@/pages/guides/ReleaseTile.astro";
import Tile from "@/pages/guides/Tile.astro";
import { Icon, Logo } from "@synnaxlabs/media";
import { Align, Button, Text } from "@synnaxlabs/pluto";
---

<Root frontmatter={{ title: "Guides" }}>
    <Align.Space direction="y" className="guides" el="article" grow size={10}>
        <Align.Space
            className="hero-parent"
            direction="y"
            size={10}
            justify="center"
            align="center"
        >
            <Align.Space className="hero" direction="x" size={10}>
                <Align.Space direction="y" grow size={6}>
                    <Align.Space direction="y" align="start" size="large">
                        <Text.Text level="h1" className="title">Synnax Docs</Text.Text>
                        <Text.Text level="p" shade={7} style={{ maxWidth: "500px" }}>
                            Guides and references to instrument and operate hardware
                            with Synnax.
                        </Text.Text>
                    </Align.Space>
                    <Align.Space direction="y" size={4} grow>
                        <Align.Space
                            className="chip guide"
                            el="a"
                            href="/guides/"
                            direction="y"
                            grow
                            bordered
                            rounded
                            align="start"
                            justify="center"
                        >
                            <Align.Space size="small">
                                <Text.WithIcon level="h4" size="medium" shade={9}>
                                    <Icon.Guide slot="startIcon" />
                                    Guides</Text.WithIcon
                                >
                                <Text.Text level="small" shade={7} weight={400}>
                                    Thorough walkthroughs to teach every aspect of the
                                    Synnax platform.
                                </Text.Text>
                            </Align.Space>
                        </Align.Space>
                        <Align.Space
                            grow
                            el="a"
                            href="/reference/"
                            bordered
                            rounded
                            align="start"
                            justify="center"
                            className="chip reference"
                        >
                            <Align.Space size="small">
                                <Text.WithIcon level="h4" shade={9} size="medium">
                                    <Icon.Reference slot="startIcon" />
                                    Reference</Text.WithIcon
                                >
                                <Text.Text level="small" shade={7} weight={400}>
                                    In-depth documentation on Synnax concepts and
                                    components.
                                </Text.Text>
                            </Align.Space>
                        </Align.Space>
                    </Align.Space>
                </Align.Space>
                <Align.Space direction="y" grow size={10}>
                    <Align.Space
                        direction="y"
                        className="get"
                        align="center"
                        justify="center"
                        bordered
                    >
                        <Logo variant="icon" color="white" />
                        <Text.Text level="p" shade={0}>Community Edition</Text.Text>
                        <Text.Text level="h3" shade={0}> Install Synnax</Text.Text>
                        <Icon.ReturnRight
                            style={{
                                color: "var(--pluto-dark-gray-l10)",
                                width: "4rem",
                                height: "4rem",
                            }}
                        />
                    </Align.Space>
                </Align.Space>
            </Align.Space>
        </Align.Space>
        <Align.Space direction="x">
            <Tile />
            <ReleaseTile />
        </Align.Space>
        <OneBillionRowsTile />
    </Align.Space>

