{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "msedge",
      "request": "attach",
      "name": "Attach to Edge",
      "port": 1422,
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "/src/*": "${workspaceFolder}/console/src/*",
        "./src/*": "${workspaceFolder}/console/src/*"
      }
    }
  ]
}
