    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.5.0",
    "eslint": "^9.0.0",
    "eslint-config-synnaxlabs": "workspace:*",
    "typescript": "^5.4.5",
    "vite": "5.2.11",
    "vitest": "^1.5.0",
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  },
  "main": "dist/client.cjs",
  "module": "dist/client.js",
  "types": "dist/index.d.ts"
