{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [
    "SYNNAX_TS_ENV",
    "TAURI_SIGNING_PRIVATE_KEY",
    "TAURI_SIGNING_PRIVATE_KEY_PASSWORD"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".vercel/**"
      ]
    },
    "lint": {},
    "dev-vite": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "watch": {
      "cache": false
    },
    "fix": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "cov": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "devtools": {
      "cache": false
    }
  }
}
