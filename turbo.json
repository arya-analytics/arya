{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {},
    "dev": {
      "cache": false
    },
    "watch": {
      "cache": false
    },
    "fix": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "cov": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "devtools": {
      "cache": false
    }
  }
}
